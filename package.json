{
  "name": "react-smart-date-compare",
  "version": "1.0.1",
  "description": "A highly customizable React date range picker with advanced comparison modes.",
  "type": "module",
  "main": "./dist/react-smart-date-compare.umd.js",
  "module": "./dist/react-smart-date-compare.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-smart-date-compare.es.js",
      "require": "./dist/react-smart-date-compare.umd.js"
    },
    "./style.css": "./dist/react-smart-date-compare.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -b tsconfig.build.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.2.0",
    "@tailwindcss/vite": "^4.0.5",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.24",
    "date-fns": "^4.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^6.0.0"
  },
  "keywords": [
    "react",
    "date-picker",
    "date-range",
    "comparison",
    "calendar",
    "date-fns",
    "tailwind"
